{
   "type": "sqlite",
   "database": "image-management-tool.db",
   "synchronize": false,
   "migrationsRun": true,
   "logging": false,
   "entities": [
      "server/entity/**/*.js"
   ],
   "migrations": [
      "server/migration/**/*.js"
   ],
   "subscribers": [
      "server/subscriber/**/*.js"
   ],
   "cli": {
      "entitiesDir": "server/entity",
      "migrationsDir": "server/migration",
      "subscribersDir": "server/subscriber"
   }
}